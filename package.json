{
  "name": "legislation-explorer",
  "version": "1.0.0",
  "description": "OpenFisca - Legislation Explorer",
  "scripts": {
    "build": "node --require esm --require dotenv/config node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.babel.js",
    "clean": "rm -rf public",
    "dev": "concurrently --kill-others 'npm run dev:app-server' 'npm run dev:webpack-dev-server'",
    "dev:app-server": "NODE_ENV=development node --require dotenv/config index.js",
    "dev:webpack-dev-server": "node --require dotenv/config --require @babel/register src/server/webpack-dev-server.js",
    "lint": "eslint --ext .js,.jsx .",
    "format": "npm run lint -- --fix",
    "start": "NODE_ENV=production node --require dotenv/config index.js",
    "test:unit": "mocha --require @babel/register tests/unit/**/*.js",
    "test:integration": "watai tests/integration",
    "test": "npm run lint && npm run test:unit && npm run test:integration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfisca/legislation-explorer.git"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@babel/node": "^7.20.5",
    "accept-language": "^3.0.17",
    "bootstrap": "^3.4.1",
    "copy-webpack-plugin": "^9.1.0",
    "diacritics": "^1.3.0",
    "dotenv": "^6.0.0",
    "express": "^4.12.4",
    "history": "^2.1.2",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "piwik-react-router": "^0.12.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^16.5.2",
    "react-highlight": "^0.12.0",
    "react-intl": "^2.7.1",
    "react-router": "^3.2.1",
    "serve-favicon": "^2.3.0",
    "swagger-ui": "~3.17.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.18.9",
    "babel-loader": "^9.1.0",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "piping": "^1.0.0-rc.4",
    "watai": "^0.7.0",
    "webpack-dev-server": "^4.11.1"
  },
  "private": true
}
