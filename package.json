{
  "name": "legislation-explorer",
  "version": "1.0.0",
  "description": "OpenFisca - Legislation Explorer",
  "scripts": {
    "build": "node --require dotenv/config node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.babel.js",
    "clean": "rm -rf public",
    "dev": "concurrently --kill-others 'npm run dev:app-server' 'npm run dev:webpack-dev-server'",
    "dev:app-server": "NODE_ENV=development node --require dotenv/config index.js",
    "dev:webpack-dev-server": "node --require dotenv/config --require babel-register src/server/webpack-dev-server.js",
    "lint": "eslint --ext .js,.jsx .",
    "start": "NODE_ENV=production node --require dotenv/config index.js",
    "test:unit": "mocha --compilers js:babel-core/register tests/unit/**/*.js",
    "test:integration": "watai tests/integration",
    "test": "npm run lint && npm run test:unit && npm run test:integration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfisca/legislation-explorer.git"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "accept-language": "^3.0.17",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.5",
    "copy-webpack-plugin": "^4.5.2",
    "diacritics": "^1.3.0",
    "dotenv": "^6.0.0",
    "express": "^4.12.4",
    "history": "^2.1.2",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "piwik-react-router": "^0.12.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-document-title": "^2.0.2",
    "react-highlight": "^0.12.0",
    "react-dom": "^16.4.2",
    "react-intl": "^2.1.2",
    "react-router": "^3.2.1",
    "serve-favicon": "^2.3.0",
    "swagger-ui": "~3.17.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "concurrently": "^3.6.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "piping": "^1.0.0-rc.3",
    "react-transform-hmr": "^1.0.4",
    "should": "^13.2.3",
    "watai": "^0.7.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-error-notification": "^0.1.8"
  },
  "private": true
}
