{
  "name": "legislation-explorer",
  "version": "0.0.1",
  "description": "OpenFisca - Legislation Explorer",
  "scripts": {
    "build": "webpack --config ./webpack.config.prod.babel.js",
    "build:tunisia": "COUNTRY_PRODUCTION_CONFIG=tunisia webpack --config ./webpack.config.prod.babel.js",
    "clean": "rm -rf public",
    "dev": "concurrently --kill-others 'npm run dev:app-server' 'npm run dev:webpack-dev-server'",
    "dev:app-server": "NODE_ENV=development PORT=2030 node index.js",
    "dev:webpack-dev-server": "WEBPACK_PORT=2031 babel-node src/server/webpack-dev-server.js",
    "dev:prod-api": "API_URL=https://fr.openfisca.org/api/v18 npm run dev",
    "lint": "eslint --ext .js,.jsx .",
    "start": "NODE_ENV=production PORT=2030 node index.js",
    "test:unit": "mocha --compilers js:babel-core/register tests/unit/**/*.js",
    "test:integration": "watai tests/integration",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "openfisca": "openfisca serve -p 2000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfisca/legislation-explorer.git"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "accept-language": "^3.0.17",
    "babel-cli": "^6.11.4",
    "bootstrap": "^3.3.5",
    "classnames": "^2.2.5",
    "diacritics": "^1.3.0",
    "express": "^4.12.4",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "piwik-react-router": "^0.4.0",
    "ramda": "^0.23.0",
    "react": "^15.1.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.1.0",
    "react-highlight": "^0.10.0",
    "react-intl": "^2.1.2",
    "react-router": "^2.4.1",
    "serve-favicon": "^2.3.0",
    "swagger-ui": "3.0.15",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "enzyme": "2.9.1",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "piping": "^1.0.0-rc.3",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "react-transform-hmr": "^1.0.4",
    "should": "^11.2.1",
    "watai": "^0.7.0",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0",
    "webpack-error-notification": "^0.1.4"
  },
  "private": true
}
